{
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/src/index.js",
    "./wc": "./dist/src/<%= tagName %>.js"
  },
  "scripts": {
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
    "build": "rm -rf ./dist && tsc && <%= scriptRunCommand %> analyze -- --exclude dist",
    "prepublish": "rm -rf ./dist && tsc && <%= scriptRunCommand %> analyze -- --exclude dist"
  },
  "dependencies": {
    "lit": "^2.0.2"
  },
  "devDependencies": {
    "@web/dev-server": "^0.1.34",
    "concurrently": "^5.3.0",
    "typescript": "^4.5.2",
    "tslib": "^2.3.1"
  }
}
